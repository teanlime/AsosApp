<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.teanlime.asosapp">

    <application
        android:name=".base.presentation.AsosApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.teanlime.asosapp.utils.imageloading.glide.AsosGlideModule"
            android:value="GlideModule"/>

        <activity
            android:name=".loading.presentation.SplashActivity"
            android:noHistory="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".home.presentation.HomeActivity"/>
    </application>

</manifest>